{
    "name": "twistor/flysystem-encrypt",
    "description": "Transparently encrypts/decrypts files for Flysystem.",
    "keywords": [
        "league"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/twistor/flysystem-encrypt",
    "authors": [
        {
            "name": "Chris Leppanen",
            "email": "chris.leppanen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "defuse/php-encryption": "~2.0",
        "league/flysystem": "~1.0",
        "twistor/flysystem-passthrough-adapter": "~1.0"
    },
    "require-dev": {
        "league/flysystem-memory": "^1.0",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Twistor\\Flysystem\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "defuse/php-encryption",
                "version": "2.0.0",
                "source": {
                    "url": "https://github.com/defuse/php-encryption.git",
                    "type": "git",
                    "reference": "v2.0"
                }
            }
        }
    ]
}
