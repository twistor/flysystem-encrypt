{
    "name": "twistor/flysystem-encrypt",
    "description": "Transparently encrypts/decrypts files for Flysystem.",
    "keywords": [
        "league"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/twistor/flysystem-encrypt",
    "authors": [
        {
            "name": "Chris Leppanen",
            "email": "chris.leppanen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "defuse/php-encryption": "~1.0",
        "league/flysystem": "~1.0",
        "twistor/flysystem-passthrough-adapter": "~1.0"
    },
    "require-dev": {
        "league/flysystem-memory": "^1.0",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Twistor\\Flysystem\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
